    <operator id="YuleModelSubtreeSlide.t:beast" spec="SubtreeSlide" tree="@Tree.t:beast" weight="15.0"/>

    <operator id="YuleModelNarrow.t:beast" spec="Exchange" tree="@Tree.t:beast" weight="15.0"/>

    <operator id="YuleModelWide.t:beast" spec="Exchange" isNarrow="false" tree="@Tree.t:beast" weight="3.0"/>

    <operator id="YuleModelWilsonBalding.t:beast" spec="WilsonBalding" tree="@Tree.t:beast" weight="3.0"/>

    <operator id="ucldStdevScaler.c:beast" spec="ScaleOperator" parameter="@ucldStdev.c:beast" scaleFactor="0.5" weight="3.0"/>

    <operator id="CategoriesRandomWalk.c:beast" spec="IntRandomWalkOperator" parameter="@rateCategories.c:beast" weight="10.0" windowSize="1"/>

    <operator id="CategoriesSwapOperator.c:beast" spec="SwapOperator" intparameter="@rateCategories.c:beast" weight="10.0"/>

    <operator id="CategoriesUniform.c:beast" spec="UniformOperator" parameter="@rateCategories.c:beast" weight="10.0"/>

