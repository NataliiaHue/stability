<run id="mcmc" spec="MCMC" chainLength="10000000">
    <state id="state" storeEvery="5000">
        <tree id="Tree.t:beast" name="stateNode">
            <taxonset id="TaxonSet.beast" spec="TaxonSet">
                <alignment idref="beast"/>
            </taxonset>
        </tree>
        <parameter id="birthRate.t:beast" name="stateNode">1.0</parameter>
        <parameter id="ucldStdev.c:beast" lower="0.0" name="stateNode">0.1</parameter>
        <stateNode id="rateCategories.c:beast" spec="parameter.IntegerParameter" dimension="118">1</stateNode>
    </state>

    <init id="RandomTree.t:beast" spec="beast.evolution.tree.RandomTree" estimate="false" initial="@Tree.t:beast" taxa="@beast">
        <populationModel id="ConstantPopulation0.t:beast" spec="ConstantPopulation">
            <parameter id="randomPopSize.t:beast" name="popSize">1.0</parameter>
        </populationModel>
    </init>

    <distribution id="posterior" spec="util.CompoundDistribution">
        <distribution id="prior" spec="util.CompoundDistribution">
            <distribution id="YuleModel.t:beast" spec="beast.evolution.speciation.YuleModel" birthDiffRate="@birthRate.t:beast" tree="@Tree.t:beast"/>
            <prior id="YuleBirthRatePrior.t:beast" name="distribution" x="@birthRate.t:beast">
                <Uniform id="Uniform.1" name="distr"/>
            </prior>
            <prior id="ucldStdevPrior.c:beast" name="distribution" x="@ucldStdev.c:beast">
                <Gamma id="Gamma.0" name="distr">
                    <parameter id="RealParameter.3" estimate="false" name="alpha">0.5396</parameter>
                    <parameter id="RealParameter.4" estimate="false" name="beta">0.3819</parameter>
                </Gamma>
            </prior>
        </distribution>
        <distribution id="likelihood" spec="util.CompoundDistribution" useThreads="true">
            <distribution id="treeLikelihood.beast" spec="ThreadedTreeLikelihood" data="@beast" tree="@Tree.t:beast">
                <siteModel id="SiteModel.s:beast" spec="SiteModel">
                    <parameter id="mutationRate.s:beast" estimate="false" name="mutationRate">1.0</parameter>
                    <parameter id="gammaShape.s:beast" estimate="false" name="shape">1.0</parameter>
                    <parameter id="proportionInvariant.s:beast" estimate="false" lower="0.0" name="proportionInvariant" upper="1.0">0.0</parameter>
                    <substModel id="MutationDeathModel.s:beast" spec="MutationDeathModel">
                        <parameter id="deathprob.s:beast" estimate="false" name="deathprob">0.1</parameter>
                        <frequencies id="freqs.s:beast" spec="Frequencies">
                            <parameter id="RealParameter.1" dimension="2" name="frequencies">1.0 0.0</parameter>
                        </frequencies>
                    </substModel>
                </siteModel>
                <branchRateModel id="RelaxedClock.c:beast" spec="beast.evolution.branchratemodel.UCRelaxedClockModel" rateCategories="@rateCategories.c:beast" tree="@Tree.t:beast">
                    <LogNormal id="LogNormalDistributionModel.c:beast" S="@ucldStdev.c:beast" meanInRealSpace="true" name="distr">
                        <parameter id="RealParameter.2" estimate="false" lower="0.0" name="M" upper="1.0">1.0</parameter>
                    </LogNormal>
                    <parameter id="ucldMean.c:beast" estimate="false" name="clock.rate">1.0</parameter>
                </branchRateModel>
            </distribution>
        </distribution>
    </distribution>

    <operator id="YuleBirthRateScaler.t:beast" spec="ScaleOperator" parameter="@birthRate.t:beast" scaleFactor="0.75" weight="3.0"/>

    <operator id="YuleModelTreeScaler.t:beast" spec="ScaleOperator" scaleFactor="0.5" tree="@Tree.t:beast" weight="3.0"/>

    <operator id="YuleModelTreeRootScaler.t:beast" spec="ScaleOperator" rootOnly="true" scaleFactor="0.5" tree="@Tree.t:beast" weight="3.0"/>

